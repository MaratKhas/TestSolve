@model Ways
@{
    ViewData["Title"] = "Результат";
}
<h1>Результат</h1>

@{
    int[,] way = Model.way;
    int Range = way.GetLength(0);
    for( int i  = 0; i < Range; i++)
    {
        for(int j = 0 ; j < Range ; j++)
        {
            if(way[i,j] == 1)
            {
                int post = i + 1;
                int get = j + 1;
                <p>Студент @post отправил сообщение студенту @get</p>
               }
           }
       }
    <p>Все студенты уведомлены о тесте</p>
}

